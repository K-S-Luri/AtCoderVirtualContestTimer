{
    "manifest_version": 2,
    "name": "extension test",
    "short_name": "extest",
    "version": "0.0.0.0",
    "icons": {
        "16": "images/icon/icon_16.png",
        "48": "images/icon/icon_48.png",
        "128": "images/icon/icon_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://not-522.appspot.com/contest/*"
            ],
            "js": [
                "jquery.js",
                "content-script.js"
            ]
        }
    ],
    "permissions": ["storage", "alarms"],
    "browser_action": {
        "default_icon": {
            "19": "images/icon/icon_19.png",
            "38": "images/icon/icon_38.png"
        },
        "default_title": "Ext title"
    }
}
